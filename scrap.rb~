
require 'rubygems'
require 'mechanize'

a = Mechanize.new { |agent|
  agent.user_agent_alias = 'Mac Safari'
}

a.get('http://google.com.vn/') do |page|
  search_result = page.form_with(:name => 'q') do |search|
   puts search
  end.submit

  search_result.links.each do |link|
    puts link.text
  end
end
